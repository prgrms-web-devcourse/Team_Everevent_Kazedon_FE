{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",     
    [
      "module-resolver", {
        "alias": {
          "@components": "./src/components",
          "@hooks": "./src/hooks"
        }
      }
    ]
  ]
}
